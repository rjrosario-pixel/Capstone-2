{
  "manifest_version": 3,
  "name": "Phishing Scanner",
  "version": "1.0",
  "description": "Scan URLs to check for phishing threats.",
  "action": {
    "default_popup": "Scanner.html",
    "default_icon": {
      "16": "images/logo (1).png",
      "48": "images/logo (1).png",
      "128": "images/logo (1).png"
    }
  },
  "permissions": ["activeTab"],
  "host_permissions": ["http://127.0.0.1:5000/"]
}
